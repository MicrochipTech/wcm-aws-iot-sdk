#include "framework/driver/adc/drv_adc_static.h"
 
<#if CONFIG_USE_DRV_ADC == true>
<#if CONFIG_DRV_ADC_DRIVER_MODE == "STATIC">
// *****************************************************************************
// *****************************************************************************
// Section: ADC Static Driver Functions
// *****************************************************************************
// *****************************************************************************
void DRV_ADC_Initialize(void)
{
    /* Select Clock Source */
    PLIB_ADC_ConversionClockSourceSelect(DRV_ADC_ID_1, ${CONFIG_DRV_ADC_CLK_SOURCE_SELECT});
    /* Select Clock Prescaler */
<#if CONFIG_DRV_ADC_CLK_SOURCE_SELECT == "ADC_CLOCK_SOURCE_SYSTEM_CLOCK">	
    PLIB_ADC_ConversionClockSet(DRV_ADC_ID_1, SYS_CLK_FREQ, ${CONFIG_DRV_ADC_CLK_VALUE_SELECT});
<#else>
    PLIB_ADC_ConversionClockSet(DRV_ADC_ID_1, SYS_CLK_CONFIG_PRIMARY_XTAL, ${CONFIG_DRV_ADC_CLK_VALUE_SELECT});
</#if>

    /* Select Power Mode */
<#if CONFIG_DRV_ADC_POWER_STATE == "SYS_MODULE_POWER_IDLE_STOP">
    PLIB_ADC_StopInIdleEnable(DRV_ADC_ID_1);
<#else>
    PLIB_ADC_StopInIdleDisable(DRV_ADC_ID_1);
</#if>
<#if CONFIG_DRV_ADC_OFFSET_CALIBRATION == true>

    /* Enable Calibration */
    PLIB_ADC_CalibrationEnable(DRV_ADC_ID_1);
</#if>
	
    /* Select Voltage Reference */
<#if CONFIG_DRV_ADC_VOLTAGE_REFERENCE_ADC?has_content>	
    PLIB_ADC_VoltageReferenceSelect(DRV_ADC_ID_1, ${CONFIG_DRV_ADC_VOLTAGE_REFERENCE_ADC});
</#if>

    /* Sampling Selections */
<#if CONFIG_DRV_ADC_AUTO_SAMPLE_EN ==true>
    /* Enable Auto Sample Mode */
    PLIB_ADC_SampleAutoStartEnable(DRV_ADC_ID_1);
<#if CONFIG_DRV_ADC_ACQUISITION_TIME?has_content>	
    /* Sample Acquisition Time (Auto Sample Mode) */	
    PLIB_ADC_SampleAcquisitionTimeSet(DRV_ADC_ID_1, ${CONFIG_DRV_ADC_ACQUISITION_TIME});
</#if>
<#if CONFIG_DRV_ADC_STOP_CONVERSION == true>
    /* Stop Conversion Sequence on First Interrupt (Auto Sample Mode) */
    PLIB_ADC_ConversionStopSequenceEnable(DRV_ADC_ID_1);
</#if>
</#if>
<#if CONFIG_DRV_ADC_ALTS_MODE?has_content>
    /* Select Sampling Mode */
    PLIB_ADC_SamplingModeSelect(DRV_ADC_ID_1, ${CONFIG_DRV_ADC_ALTS_MODE});
</#if>
<#if CONFIG_DRV_ADC_NUMBER_OF_SAMPLES?has_content>
    /* Number of Samples Per Interrupt */
    PLIB_ADC_SamplesPerInterruptSelect(DRV_ADC_ID_1, ${CONFIG_DRV_ADC_NUMBER_OF_SAMPLES});
</#if>

    /* Conversion Selections */
<#if CONFIG_DRV_ADC_TRIG_SRC?has_content>
    /* Select Trigger Source */
    PLIB_ADC_ConversionTriggerSourceSelect(DRV_ADC_ID_1, ${CONFIG_DRV_ADC_TRIG_SRC});
</#if>
<#if CONFIG_DRV_ADC_OUTPUT_FOMRAT?has_content>
    /* Select Result Format */
    PLIB_ADC_ResultFormatSelect(DRV_ADC_ID_1, ${CONFIG_DRV_ADC_OUTPUT_FOMRAT});
</#if>
<#if CONFIG_DRV_ADC_BUFFER_RESULT_MODE?has_content>
    /* Buffer Mode */
    PLIB_ADC_ResultBufferModeSelect(DRV_ADC_ID_1, ${CONFIG_DRV_ADC_BUFFER_RESULT_MODE});
</#if>

    /* Channel Selections */
<#if CONFIG_DRV_ADC_ALTS_MODE == "ADC_SAMPLING_MODE_MUXA">
<#if CONFIG_DRV_ADC_MUXA_NEG_INPUT?has_content>
    /* MUX A Negative Input Select */
    PLIB_ADC_MuxChannel0InputNegativeSelect(DRV_ADC_ID_1, ADC_MUX_A, ${CONFIG_DRV_ADC_MUXA_NEG_INPUT});
</#if>
<#else>
<#if CONFIG_DRV_ADC_MUXA_NEG_INPUT?has_content>

    /* MUX A Negative Input Select */
    PLIB_ADC_MuxChannel0InputNegativeSelect(DRV_ADC_ID_1, ADC_MUX_A, ${CONFIG_DRV_ADC_MUXA_NEG_INPUT});
</#if>
<#if CONFIG_DRV_ADC_MUXB_NEG_INPUT?has_content>

    /* MUX B Negative Input Select */
    PLIB_ADC_MuxChannel0InputNegativeSelect(DRV_ADC_ID_1, ADC_MUX_B, ${CONFIG_DRV_ADC_MUXB_NEG_INPUT});
</#if>
</#if>
<#macro ADC_CHANNEL_SCAN_FUNCTION SCAN_INST SCAN_ENABLE SCAN_CHN_NUM ESCAN_ENABLE ESCAN_CHN_NUM
MUX_SEL_A MUX_SEL_B DEDICATED_CHN_NUM>
<#if CONFIG_DRV_ADC_SCAN_MODE == true>
<#if SCAN_ENABLE == false>
<#if ESCAN_ENABLE == false>
<#if CONFIG_DRV_ADC_ALTS_MODE == "ADC_SAMPLING_MODE_MUXA">

    /* MUX A Positive Input Select */
    PLIB_ADC_MuxChannel0InputPositiveSelect(DRV_ADC_ID_1, ADC_MUX_A, ${DEDICATED_CHN_NUM});
<#else>
<#if MUX_SEL_A == true>

    /* MUX A Positive Input Select */
    PLIB_ADC_MuxChannel0InputPositiveSelect(DRV_ADC_ID_1, ADC_MUX_A, ${DEDICATED_CHN_NUM});
</#if>
<#if MUX_SEL_B == true>

    /* MUX B Positive Input Select */
