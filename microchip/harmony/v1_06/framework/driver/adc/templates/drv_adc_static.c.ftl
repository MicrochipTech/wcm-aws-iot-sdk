ADC_SAMPLE DRV_ADC_SamplesRead(uint8_t bufIndex)
{
    /* Read Result */
    return PLIB_ADC_ResultGetByIndex(DRV_ADC_ID_1, bufIndex);
}

bool DRV_ADC_SamplesAvailable(void)
{
    /* Wait for DONE Flag */
    while (!PLIB_ADC_ConversionHasCompleted(DRV_ADC_ID_1));
	
    return true;
}
