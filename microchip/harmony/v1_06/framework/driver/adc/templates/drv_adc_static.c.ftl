inline void DRV_ADC_ChannelExtendedScanInputsAdd(DRV_ADC_INPUTS_SCAN_EXTENDED eScanInput)
{
    /* Add Scan Input */
    PLIB_ADC_InputScanMaskAddExtended(DRV_ADC_ID_1, eScanInput);
}

inline void DRV_ADC_ChannelExtendedScanInputsRemove(DRV_ADC_INPUTS_SCAN_EXTENDED eScanInput)
{
    /* Remove Scan Input */
    PLIB_ADC_InputScanMaskRemoveExtended(DRV_ADC_ID_1, eScanInput);
