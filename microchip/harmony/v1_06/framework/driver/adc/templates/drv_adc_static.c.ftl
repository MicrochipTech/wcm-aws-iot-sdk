</#if>
</#if>
<#else> <#--ESCAN_ENABLE == false-->

    /* Select Extended Scan Input ${SCAN_INST} */
    PLIB_ADC_InputScanMaskAddExtended(DRV_ADC_ID_1, ${ESCAN_CHN_NUM});
</#if> <#--ESCAN_ENABLE == false-->
<#else> <#--SCAN_ENABLE == false-->
    /* Select Scan Input ${SCAN_INST} */
